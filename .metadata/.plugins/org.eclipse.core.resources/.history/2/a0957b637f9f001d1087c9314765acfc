import java.util.Scanner;

public class Baek_1032명령프롬프트 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		String t = sc.next();
		int len = t.length();
		char[] titleArr = new char[len];
		for (int i=1; i<N; i++) {
			String c = sc.next();
			for(int j=0; j<len; j++) {
				titleArr[i] = t.charAt(j);
				
				if(c.charAt(j)== titleArr[j]) {
					//여기서 char != char로 했는데 왜 안됐을까..
				}else {
					titleArr[j] = "?".charAt(0);
					System.out.println(titleArr[j]);
				}
			}
			
			
		}
		for(int j=0; j<len; j++) {
			System.out.print(titleArr[j]);
		}
		
		}

}
