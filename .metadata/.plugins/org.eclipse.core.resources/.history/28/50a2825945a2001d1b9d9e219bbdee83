package D2;

import java.util.Scanner;

public class SE1946_간단압축풀기 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		int cnt = 0;
		int idx = 0;
		char[] printArr = new char[10];
		//테스트케이스만큼 반복 시작
		for(int t=1; t<=T; t++) {
			System.out.println("#"+ t);
			//N 입력 받기
			int N = sc.nextInt();
			for(int n=0; n<N; n++) {
				char ch = sc.next().charAt(0);
				int zipNum = sc.nextInt();
				for(int i=0; i<zipNum; i++) {
					if(i<10) {
						printArr[idx++] = ch;
						if(idx==9) {
							for(int j=0; j<9; j++) {
								System.out.print(printArr[i]);
								printArr[j] = "".charAt(0);
								idx = 0;
							}
							System.out.println();
						}
					}else if(i>10) {
						printArr[idx++] = ch;
						if(idx==9) {
							for(int j=0; j<9; j++) {
								System.out.print(printArr[i]);
								printArr[j] = "".charAt(0);
								idx = 0;
							}
							System.out.println();
						}
							
						}
					
						
					}
				}
			}
		}

	}

}
