import java.util.Scanner;

public class Baek_1032명령프롬프트 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		String t = sc.next();
		int len = t.length();
		char[] titleArr = new char[len];
		for (int i=1; i<N; i++) {
			String c = sc.next();
			for(int j=0; j<len; j++) {
				titleArr[i] = t.charAt(i);
				if(titleArr[i] != c.charAt(i)) {
					titleArr[i] = "?".charAt(0);
				}
			}
		}
		for(int i=0; i<len; i++) {
			System.out.print(titleArr[i]);
		}
		
		
		}

}
