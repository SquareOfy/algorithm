			int k = sc.nextInt();
			int n = sc.nextInt();
			int sum = 0;
			int[] numArr = new int[k+1];
			for(int i=1; i<=k; i++) {
				numArr[i] += i;
			}
			for(int i=1; i<=n; i++) {
				sum = 0;
				for(int i=k; k>0; k++ ) {
					sum 
				}
			}
			
		}
		
		
	}

}
